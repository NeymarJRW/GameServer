how = false;\\n          if (res.msg == \\\"数据为空！\\\") {\\n            this.getpage(\\\"\\\");\\n            this.spinShow = true;\\n             this.procon = []\\n          } else {\\n            var len = res.msg.length;\\n            for (var i = 0; i < len; i++) {\\n              //获取游戏分类\\n              if (this.gametype.indexOf(res.msg[i].class.name) == -1) {\\n                this.gametype.push(res.msg[i].class.name);\\n              }\\n              var tip_len = res.msg[i].label.length;\\n              for (var j = 0; j < tip_len; j++) {\\n                if (this.gametip.indexOf(res.msg[i].label[j].name) == -1) {\\n                  this.gametip.push(res.msg[i].label[j].name);\\n                }\\n              }\\n              this.procon.push(res.msg[i].tab)\\n            }\\n            this.getpage(res.msg);\\n            // console.log(this.procon)\\n            \\n          }\\n        }else{\\n           this.getpage(\\\"\\\");\\n           this.spinShow = false;\\n           this.procon.push(res.msg[i].tab)\\n        }\\n      });\\n    },\\n\\n    //全部游戏列表\\n    allgame() {\\n      this.pageindex=1\\n      gamePending().then(res => {\\n        this.showtype = [];\\n        this.showtip = [];\\n        this.showpingtai=[]\\n        if (res.state === \\\"1\\\") {\\n          this.getpage(res.msg);\\n          this.spinShow = false;\\n        }\\n      });\\n    },\\n    //选择平台\\n    selectpigntai(i){\\n      this.pageindex=1\\n      this.showtype = [];\\n      this.showtip = [];\\n      this.showpingtai=i\\n      gameIndex().then(res => {\\n        if (res.state === \\\"1\\\") {\\n          var newarr = res.msg.filter(function(item) { \\n            // console.log(i)\\n            if(i=='小程序'){\\n              if (item.wechat == '1') {\\n              return item\\n              }\\n            }\\n            else if(i=='安卓'){\\n              if (item.android == '1') {\\n              return item\\n              }\\n            }else if(i=='ios